class Productsum 
{
	public static void main(String[] args)
	{
	int m=1;
	int n=5;
	int sum=0;
	int prod=1;
	for (int i=m;i<=n;i++)
	{
		sum=sum+i;
		prod=prod*i;
	}
		System.out.println("the summation and product of natural numbers between mandnis"+sum);
		System.out.println("the summation and product of natural numbers between mandnis"+prod);
	}
}
